<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
	elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:element name="cooperation">
		<xs:annotation>
			<xs:documentation>根节点</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:all>
				<xs:element name="application">
					<xs:annotation>
						<xs:documentation>申请人信息</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:all>
							<xs:element name="id">
								<xs:annotation>
									<xs:documentation>申请人ID（在网上审批大厅自动生成）</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:minLength value="10" />
										<xs:maxLength value="30" />
										<xs:whiteSpace value="replace" />
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="appid">
								<xs:annotation>
									<xs:documentation>申请人信息ID（申请人在网上审批大厅注册的用户ID）</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:minLength value="10" />
										<xs:maxLength value="50" />
										<xs:whiteSpace value="replace" />
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="appname">
								<xs:annotation>
									<xs:documentation>申请人姓名</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:minLength value="0" />
										<xs:maxLength value="200" />
										<xs:whiteSpace value="replace" />
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="apporg">
								<xs:annotation>
									<xs:documentation>申请单位姓名</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:minLength value="0" />
										<xs:maxLength value="200" />
										<xs:whiteSpace value="replace" />
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="cardid">
								<xs:annotation>
									<xs:documentation>申请人身份证号</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:whiteSpace value="replace" />
										<xs:minLength value="0" />
										<xs:maxLength value="30" />
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="appdate">
								<xs:annotation>
									<xs:documentation>申请时间</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:dateTime" />
								</xs:simpleType>
							</xs:element>
							<xs:element name="mobilephone">
								<xs:annotation>
									<xs:documentation>申请人手机号码</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:minLength value="0" />
										<xs:maxLength value="11" />
										<xs:whiteSpace value="replace" />
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="phone">
								<xs:annotation>
									<xs:documentation>申请人固定电话</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:minLength value="0" />
										<xs:maxLength value="13" />
										<xs:whiteSpace value="replace" />
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="email">
								<xs:annotation>
									<xs:documentation>申请人邮箱</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:minLength value="0" />
										<xs:maxLength value="50" />
										<xs:whiteSpace value="replace" />
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="address">
								<xs:annotation>
									<xs:documentation>申请人地址</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:minLength value="0" />
										<xs:maxLength value="500" />
										<xs:whiteSpace value="replace" />
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="usertype">
								<xs:annotation>
									<xs:documentation>用户类型，1个人用户，2企业用户（补充）</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:whiteSpace value="replace" />
										<xs:minLength value="0" />
										<xs:maxLength value="1" />
										<xs:enumeration value="1" />
										<xs:enumeration value="2" />
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="username">
								<xs:annotation>
									<xs:documentation>用户名</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:minLength value="0" />
										<xs:maxLength value="50" />
										<xs:whiteSpace value="replace" />
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="password">
								<xs:annotation>
									<xs:documentation>用户密码（补充）</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:minLength value="0" />
										<xs:maxLength value="50" />
										<xs:whiteSpace value="replace" />
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
						</xs:all>
					</xs:complexType>
				</xs:element>
				<xs:element name="agency">
					<xs:annotation>
						<xs:documentation>代办人信息</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:all>
							<xs:element name="id">
								<xs:annotation>
									<xs:documentation>代办人ID（在网上审批大厅自动生成）</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:minLength value="0" />
										<xs:maxLength value="30" />
										<xs:whiteSpace value="replace" />
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="agencyname">
								<xs:annotation>
									<xs:documentation>代办人姓名</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:whiteSpace value="replace" />
										<xs:minLength value="0" />
										<xs:maxLength value="20" />
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="cardid">
								<xs:annotation>
									<xs:documentation>代办人身份证号</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:minLength value="0" />
										<xs:maxLength value="18" />
										<xs:whiteSpace value="replace" />
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="mobilephone">
								<xs:annotation>
									<xs:documentation>代办人手机号码</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:minLength value="0" />
										<xs:maxLength value="11" />
										<xs:whiteSpace value="replace" />
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="email">
								<xs:annotation>
									<xs:documentation>代办人邮箱</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:minLength value="0" />
										<xs:maxLength value="50" />
										<xs:whiteSpace value="replace" />
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
						</xs:all>
					</xs:complexType>
				</xs:element>
				<xs:element name="parallel" minOccurs="0">
					<xs:annotation>
						<xs:documentation>并联审批信息</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:all>
							<xs:element name="cid">
								<xs:annotation>
									<xs:documentation>并联事项ID</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:minLength value="0" />
										<xs:maxLength value="30" />
										<xs:whiteSpace value="replace" />
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="cbsnum">
								<xs:annotation>
									<xs:documentation>并联事项业务流水号</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:minLength value="0" />
										<xs:maxLength value="30" />
										<xs:whiteSpace value="replace" />
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="cprojectname">
								<xs:annotation>
									<xs:documentation>并联事项项目名称</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:minLength value="0" />
										<xs:maxLength value="100" />
										<xs:whiteSpace value="replace" />
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="cstartdate">
								<xs:annotation>
									<xs:documentation>开始时间</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:dateTime" />
								</xs:simpleType>
							</xs:element>
							<xs:element name="cenddate">
								<xs:annotation>
									<xs:documentation>结束时间</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:dateTime" />
								</xs:simpleType>
							</xs:element>
							<xs:element name="cstatus">
								<xs:annotation>
									<xs:documentation>状态（0:待办; 1:办理中; 2:办结）</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:minLength value="0" />
										<xs:maxLength value="1" />
										<xs:whiteSpace value="replace" />
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
						</xs:all>
					</xs:complexType>
				</xs:element>
				<xs:element ref="permissions" />
			</xs:all>
		</xs:complexType>
	</xs:element>
	<xs:element name="permissions">
		<xs:annotation>
			<xs:documentation>审批数据信息集节点，该节点为XMLDOC字段的核心节点</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence maxOccurs="unbounded">
				<xs:element name="permission">
					<xs:annotation>
						<xs:documentation>审批数据信息</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:all>
							<xs:element name="id">
								<xs:annotation>
									<xs:documentation>事项ID</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:minLength value="5" />
										<xs:maxLength value="30" />
										<xs:whiteSpace value="replace" />
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="name">
								<xs:annotation>
									<xs:documentation>事项名称</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:minLength value="1" />
										<xs:maxLength value="300" />
										<xs:whiteSpace value="replace" />
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="handler">
								<xs:annotation>
									<xs:documentation>是否一体化平台处理（0为一体化系统，1为否）</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:minLength value="0" />
										<xs:maxLength value="1" />
										<xs:whiteSpace value="replace" />
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="bsid">
								<xs:annotation>
									<xs:documentation>业务ID（一体化申报时产生的主键）</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:minLength value="8" />
										<xs:maxLength value="30" />
										<xs:whiteSpace value="replace" />
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="bsnum">
								<xs:annotation>
									<xs:documentation>业务流水号（一体化申报时提供的业务流水号，供申请人查询用）
									</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:minLength value="5" />
										<xs:maxLength value="30" />
										<xs:whiteSpace value="replace" />
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="regionarea">
								<xs:annotation>
									<xs:documentation>前置行政区街道</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:minLength value="6" />
										<xs:maxLength value="10" />
										<xs:whiteSpace value="replace" />
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="cid">
								<xs:annotation>
									<xs:documentation>并联审批事项ID</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:minLength value="0" />
										<xs:maxLength value="30" />
										<xs:whiteSpace value="replace" />
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="cbsnum">
								<xs:annotation>
									<xs:documentation>并联事项业务流水号（补充）</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:minLength value="0" />
										<xs:maxLength value="30" />
										<xs:whiteSpace value="replace" />
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="department">
								<xs:annotation>
									<xs:documentation>部门组织机构代码</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:minLength value="0" />
										<xs:maxLength value="30" />
										<xs:whiteSpace value="replace" />
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="flgdgxd">
								<xs:annotation>
									<xs:documentation>法律规定管辖地(法律规定管辖地的行政区划代码</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:minLength value="0" />
										<xs:maxLength value="20" />
										<xs:whiteSpace value="replace" />
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="fsywgxd">
								<xs:annotation>
									<xs:documentation>发生业务管辖地(发生业务管辖地的行政区划代码</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:minLength value="0" />
										<xs:maxLength value="20" />
										<xs:whiteSpace value="replace" />
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="sljtdd">
								<xs:annotation>
									<xs:documentation>受理具体地点(受理单位承办此受理业务人员所在的办公地点，精确到房间号或者窗口号)
									</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:minLength value="0" />
										<xs:maxLength value="20" />
										<xs:whiteSpace value="replace" />
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="tjfs">
								<xs:annotation>
									<xs:documentation>此笔业务提交方式（0-窗口提交(现场申办) 1-网上提交(网上大厅) 2-信函3-电报
										4-电传 5-传真 6-邮件 7-电子数据交换 8-窗口协办 9 - 村居代办 10 - 自助终端 11 -
										移动终端)（补充）</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:minLength value="0" />
										<xs:maxLength value="20" />
										<xs:whiteSpace value="replace" />
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="isfinish">
								<xs:annotation>
									<xs:documentation>业务状态（0:未预审;1:预审未通过;2:在办;3:办结;4:预审通过;5:受理不通过;6:办理不通过;7:补交挂起;8:特别程序挂起;-1:业务无效）
									</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:minLength value="1" />
										<xs:maxLength value="1" />
										<xs:whiteSpace value="replace" />
										<xs:enumeration value="-1" />
										<xs:enumeration value="0" />
										<xs:enumeration value="1" />
										<xs:enumeration value="2" />
										<xs:enumeration value="3" />
										<xs:enumeration value="4" />
										<xs:enumeration value="5" />
										<xs:enumeration value="6" />
										<xs:enumeration value="7" />
										<xs:enumeration value="8" />
										<xs:enumeration value="9" />
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="stagebh">
								<xs:annotation>
									<xs:documentation>阶段编号（针对并联审批，由一体化平台提供）</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:minLength value="0" />
										<xs:maxLength value="50" />
										<xs:whiteSpace value="replace" />
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="status">
								<xs:annotation>
									<xs:documentation>阶段激活状态(0未激活，1激活，针对并联审批)</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:minLength value="0" />
										<xs:maxLength value="1" />
										<xs:whiteSpace value="replace" />
										<xs:enumeration value="0" />
										<xs:enumeration value="1" />
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="certificate" minOccurs="0">
								<xs:annotation>
									<xs:documentation>证照信息</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:all>
										<xs:element name="head">
											<xs:annotation>
												<xs:documentation>证照基本信息</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:all>
													<xs:element name="id">
														<xs:annotation>
															<xs:documentation>证照id</xs:documentation>
														</xs:annotation>
														<xs:simpleType>
															<xs:restriction base="xs:string">
																<xs:minLength value="0" />
																<xs:maxLength value="20" />
																<xs:whiteSpace value="replace" />
